{
  "name": "bandmate",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
  "dev": "concurrently -n API,WEB -c yellow,cyan \"npm run dev:api\" \"npm run dev:web\"",
  "dev:web": "npm run start -w apps/web",
  "dev:api": "npm run dev -w apps/api"
},
  "devDependencies": {
    "concurrently": "^9.0.0"
  }
}
